          <table role="presentation" class="outer" align="center" cellpadding="0" cellspacing="0" border="0">
            <tr>
              <td class="no-padding">

                {% if snippet %}
                
                <!-- ======= START SNIPPET ======= -->
                <table role="presentation" width="100%" cellpadding="0" cellspacing="0" border="0" class="snippet mobile-hide">
                  <tr>
                    <td align="center">
                      <p>{% block snippet %}{% endblock %}</p>
                    </td>
                  </tr>
                </table>

                {% endif %}

                {% if header1 %}
                  {% include "partials/components/_header1.nunjucks" %}
                {% endif %}

                {% if header2 %}
                  {% include "partials/components/_header2.nunjucks" %}
                {% endif %}

                {% if header3 %}
                  {% include "partials/components/_header3.nunjucks" %}
                {% endif %}

